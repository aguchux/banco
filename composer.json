{
    "name": "bank/landmarkfinanceonline",
    "description": "Landmark Bank",
    "type": "project",
    "version": "2.0.0",
    "keywords": ["psr4", "template"],
    "license": "MIT",
    "authors": [{
        "name": "Ford Bank",
        "email": "info@landmarkfinanceonline.com",
        "homepage": "https://landmarkfinanceonline.com/",
        "role": "Developer"
    }],
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    },
    "autoload": {
        "classmap": ["_apps/", "_apps/Apps/"],
        "psr-4": {
            "Apps\\": ["_apps/", "_apps/Apps/"]
        }
    },
    "require": {
        "php": ">=5.3.0",
        "cocur/slugify": "^4.3",
        "jajo/jsondb": "^3.0",
        "mpdf/mpdf": "^8.1",
        "verot/class.upload.php": "^2.1"
    },
    "scripts": {},
    "minimum-stability": "dev",
    "prefer-stable": true,
    "bin": ["application"]
}
